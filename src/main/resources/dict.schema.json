{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "moduleInfo": {
      "$id": "/properties/moduleInfo",
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "/properties/moduleInfo/properties/name",
          "type": "string"
        },
        "needsPomSlot": {
          "$id": "/properties/moduleInfo/properties/needsPomSlot",
          "type": "boolean"
        },
        "slot": {
          "$id": "/properties/moduleInfo/properties/slot",
          "type": "string"
        }
      }
    },
    "moduleGroups": {
      "$id": "/properties/moduleGroups",
      "type": "array",
      "items": {
        "$id": "/properties/moduleGroups/items",
        "type": "object",
        "properties": {
          "groupId": {
            "$id": "/properties/moduleGroups/items/properties/groupId",
            "type": "string"
          },
          "artifactId": {
            "$id": "/properties/moduleGroups/items/properties/artifactId",
            "type": "string"
          },
          "meta-inf": {
            "$id": "/properties/moduleGroups/items/properties/meta-inf",
            "type": "string"
          }
        }
      }
    },
    "modules": {
      "$id": "/properties/modules",
      "type": "array",
      "minItems": 0,
      "items": {
        "$id": "/properties/modules/items",
        "type": "object",
        "required": [
          "groupId",
          "artifactId",
          "moduleName"
        ],
        "properties": {
          "groupId": {
            "$id": "/properties/modules/items/properties/groupId",
            "type": "string"
          },
          "artifactId": {
            "$id": "/properties/modules/items/properties/artifactId",
            "type": "string"
          },
          "version": {
            "$id": "/properties/modules/items/properties/version",
            "type": "string"
          },
          "moduleName": {
            "$id": "/properties/modules/items/properties/moduleName",
            "type": "string"
          },
          "meta-inf": {
            "$id": "/properties/modules/items/properties/meta-inf",
            "type": "string"
          },
          "export": {
            "$id": "/properties/modules/items/properties/export",
            "type": "string"
          },
          "needsPomSlot": {
            "$id": "/properties/modules/items/properties/needsPomSlot",
            "type": "boolean"
          },
          "slot": {
            "$id": "/properties/modules/items/properties/slot",
            "type": "string"
          }
        }
      }
    }
  }
}

